{
    "manifest_version": 3,
    "name": "yt-rplc",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": ["https://www.youtube.com/*"],
    "permissions": ["activeTab", "scripting", "storage", "tabs"]
}